@{ 
    demo.Models.tbl_Admin ad = (demo.Models.tbl_Admin)Session["admin"];
}
<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="au theme template">
    <meta name="keywords" content="au theme template">

    <!-- Title Page-->
    <title>Admin</title>

    <!-- Fontfaces CSS-->
    <link href="~/Content/CoolAdmin-master/css/font-face.css" rel="stylesheet" />
    <link href="~/Content/CoolAdmin-master/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">

    <!-- Bootstrap CSS-->
    <link href="~/Content/CoolAdmin-master/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/Content/CoolAdmin-master/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/Content/CoolAdmin-master/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">

    <!-- Main CSS-->
    <link href="~/Content/CoolAdmin-master/css/theme.css" rel="stylesheet" media="all">

</head>

<body class="animsition">
    <div class="page-wrapper">
        <!-- MENU SIDEBAR-->
        <aside class="menu-sidebar d-none d-lg-block">
            <div class="logo">
                <a href="#">
                    <img src="~/Content/CoolAdmin-master/images/icon/logo.png" alt="Cool Admin" />
                </a>
            </div>
            <div class="menu-sidebar__content js-scrollbar1">
                <nav class="navbar-sidebar">
                    <ul class="list-unstyled navbar__list">
                        <li>
                            <a href="@Url.Action("Home","Admin")">
                                <i class="fas fa-home"></i>Home
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Publisher")">
                                <i class="fas fa-industry"></i>Publisher
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Category")">
                                <i class="fas fa-list-ul"></i>Category
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Product")">
                                <i class="fab fa-product-hunt"></i>Product
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Bill")">
                                <i class="fas fa-calculator"></i>Bill
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Admin")">
                                <i class="fas fa-child"></i>Admin
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index","Customer")">
                                <i class="fas fa-address-card"></i>Customer
                            </a>
                        </li>
                    </ul>
                </nav>
            </div>
        </aside>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <header class="header-desktop">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="header-wrap">
                            <form class="form-header" action="" method="POST"></form>
                            <div class="header-button">
                                <div class="account-wrap">
                                    <div class="account-item clearfix js-item-menu">
                                        <div class="image">
                                            <img src="~/Content/CoolAdmin-master/images/icon/avatar-01.jpg" alt="@ad.username" />
                                        </div>
                                        <div class="content">
                                            <a class="js-acc-btn" href="#">@ad.name</a>
                                        </div>
                                        <div class="account-dropdown js-dropdown">
                                            <div class="info clearfix">
                                                <div class="image">
                                                    <a href="#">
                                                        <img src="~/Content/CoolAdmin-master/images/icon/avatar-01.jpg" alt="@ad.username" />
                                                    </a>
                                                </div>
                                                <div class="content">
                                                    <h5 class="name">
                                                        <a href="#">@ad.name</a>
                                                    </h5>
                                                    
                                                </div>
                                            </div>
                                            <div class="account-dropdown__footer">
                                                <a href="@Url.Action("Logout","Admin")">
                                                    <i class="zmdi zmdi-power"></i>Logout
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
            <!-- HEADER DESKTOP-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    @RenderBody()
                </div>
            </div>
            <!-- END PAGE CONTAINER-->
        </div>

            </div>

            <!-- Jquery JS-->
            <script src="~/Content/CoolAdmin-master/vendor/jquery-3.2.1.min.js"></script>
            <!-- Bootstrap JS-->
            <script src="~/Content/CoolAdmin-master/vendor/bootstrap-4.1/popper.min.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/bootstrap-4.1/bootstrap.min.js"></script>
            <!-- Vendor JS       -->
            <script src="~/Content/CoolAdmin-master/vendor/slick/slick.min.js">
            </script>
            <script src="~/Content/CoolAdmin-master/vendor/wow/wow.min.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/animsition/animsition.min.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
            </script>
            <script src="~/Content/CoolAdmin-master/vendor/counter-up/jquery.waypoints.min.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/counter-up/jquery.counterup.min.js">
            </script>
            <script src="~/Content/CoolAdmin-master/vendor/circle-progress/circle-progress.min.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/chartjs/Chart.bundle.min.js"></script>
            <script src="~/Content/CoolAdmin-master/vendor/select2/select2.min.js">
            </script>

            <!-- Main JS-->
            <script src="~/Content/CoolAdmin-master/js/main.js"></script>
            @RenderSection("Scripts", required: false)

</body>

</html>
<!-- end document-->
